#!/bin/sh

export PACKAGE=gnome-core
export PORTS="gconf libglade at-spi2-core gsettings-desktop-schemas \
xdg-utils xdg-user-dirs shared-mime-info desktop-file-utils"
export DROP_FILES="libglade-convert gsettings-schema-convert \
desktop-file-install desktop-file-validate desktop-file-edit"
repackage -e

returnval=$?

exit $returnval
