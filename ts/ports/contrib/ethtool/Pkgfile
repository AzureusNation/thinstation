name=ethtool
version=3.14
release=1
source=(https://www.kernel.org/pub/software/network/$name/${name%-*}-$version.tar.xz)

build() {
cd $name-$version
	./configure	--prefix=/usr \
			--disable-dependency-tracking \

	make
	make DESTDIR=$PKG install
}

