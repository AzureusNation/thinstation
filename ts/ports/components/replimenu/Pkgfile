name=replimenu
version=0.9
release=1
source=(http://downloads.sourceforge.net/project/replimenu/replimenu/0.9/replimenu-0.9.tar.gz)

build() {
	cd $name-$version
	make

        mkdir -p $PKG/usr/bin
        mv src/replimenu $PKG/usr/bin
}
