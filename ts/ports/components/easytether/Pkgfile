name=easytether
version=0.7.4
release=1
source=(https://sites.google.com/site/easytether/easytether/easytether_${version}-1_i386.deb)

build() {
	ar -x ${name}_${version}-1_i386.deb
	tar -xzvf data.tar.gz -C $PKG
}
