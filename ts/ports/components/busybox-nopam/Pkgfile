# Description: Tiny versions of many UNIX utilities into a single executable.
# URL: http://busybox.net/
# Maintainer: Donald A. Cupp Jr:doncuppjr at yahoo dot com
# Depends on:

name=busybox-nopam
cname=busybox
version=1.22.1
release=2
source=(http://busybox.net/downloads/$cname-$version.tar.bz2 ts.config)

build() {
   cd $cname-$version
   cp ../ts.config .config

	make PREFIX=/usr busybox
	make CONFIG_PREFIX=$PKG/usr install
}
