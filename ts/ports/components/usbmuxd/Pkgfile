# Description:  USB Multiplexing Daemon
# URL:          http://www.libimobiledevice.org/
# Packager:     
# Maintainer:   
# Dependencies:	libplist

name=usbmuxd
version=1.0.9
release=1
source=(http://www.libimobiledevice.org/downloads/lib$name-$version.tar.bz2)

build() {
    cd lib$name-$version
    ./configure --prefix=/usr --disable-static
    make
    make DESTDIR=$PKG install
}

