name=libpwquality
version=1.2.1
release=1
source=(https://fedorahosted.org/releases/l/i/libpwquality/libpwquality-$version.tar.bz2)

build() {
	cd $name-$version

	./configure --prefix=/usr \

	make
	make DESTDIR=$PKG install
}
