perl -MXML::SAX -e "XML::SAX->add_parser(q(XML::SAX::PurePerl))->save_parsers()" 2>/dev/null
perl -MXML::SAX -e "XML::SAX->add_parser(q(XML::SAX::Expat))->save_parsers()" 2>/dev/null
cat /usr/lib/perl5/5.18/linux-thread-multi/perllocal.pod.expat >> /usr/lib/perl5/5.18/linux-thread-multi/perllocal.pod
rm /usr/lib/perl5/5.18/linux-thread-multi/perllocal.pod.expat
